    {
      "id": "root_view",
      "type": "flex_container",
      "layout": {
        "direction": "column",
        "justify": "start",
        "align": "stretch",
        "gap": 4
      },
      "padding": {
        "top": 32,
        "right": 48,
        "bottom": 32,
        "left": 48
      },
      "background": {
        "type": "linear_gradient",
        "angle": 135,
        "stops": [
          [
            "#667eea",
            0
          ],
          [
            "#764ba2",
            1
          ]
        ]
      },
      "children": [
        "card_view"
      ]
    },
    {
      "id": "card_view",
      "type": "flex_container",
      "layout": {
        "direction": "column",
        "justify": "start",
        "align": "stretch",
        "gap": 20
      },
      "padding": {
        "top": 28,
        "right": 32,
        "bottom": 28,
        "left": 32
      },
      "background": {
        "type": "solid",
        "color": "#ffffff"
