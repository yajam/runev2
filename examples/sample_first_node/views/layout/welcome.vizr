      "corner_radius": 12,
      "children": [
        "title_text",
        "body_text",
        "image_section",
        "name_field_container",
        "mid_paragraph",
        "email_input_view",
        "message_area_view",
        "file_upload_section",
        "priority_select_view",
        "date_picker_view",
