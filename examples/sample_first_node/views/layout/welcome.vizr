    {
      "id": "mid_paragraph",
      "node_id": "MIDPARA1",
      "type": "text",
      "style": {
        "color": "#000000",
        "font_size": 15,
        "line_height": 22,
        "padding": {
          "top": 8,
          "right": 4,
          "bottom": 8,
          "left": 4
        }
      }
    },
    {
      "id": "title_text",
      "node_id": "CARDTXT1",
      "type": "text",
      "style": {
        "color": "#1e293b",
        "font_size": 32,
        "line_height": 40
      }
    },
    {
      "id": "email_input_view",
      "type": "input_box",
      "control_id": "email",
      "form_id": "contact",
      "placeholder": "Enter your email",
      "width": 320,
      "style": {
        "padding": {
          "top": 10,
          "right": 12,
          "bottom": 10,
          "left": 12
        },
        "border_width": 1,
        "border_color": "#94A3B8",
        "corner_radius": 6,
        "background": {
          "type": "solid",
          "color": "#f5f8ff"
        }
      }
    },
    {
      "id": "message_area_view",
      "type": "text_area",
      "control_id": "message",
      "form_id": "contact",
